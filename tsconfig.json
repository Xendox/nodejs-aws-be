{
  "compilerOptions": {
    "lib": ["es6"],
    "removeComments": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "types": ["aws-lambda", "node", "serverless","reflect-metadata"],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es5",
    "outDir": "lib"
  },
  "include": ["./**/*.ts"],
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ]
}
